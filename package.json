{
  "name": "playground",
  "version": "99.99.99",
  "license": "WTFPL",
  "browserslist": [
    "last 2 major versions and >1%",
    "not ie<=11",
    "not op_mini all"
  ],
  "scripts": {
    "clean": "rm -rf dist .cache",
    "build": "parcel build index.html --public-url='/' --experimental-scope-hoisting",
    "deploy": "cp assets/robots.txt dist && rsync -vrP --del dist/ `cat location`",
    "serve": "parcel",
    "s": "parcel"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@types/requestidlecallback": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "csstype": "^2.6.7",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.1",
    "posthtml-include": "^1.2.0",
    "typescript": "^3.3.3"
  }
}

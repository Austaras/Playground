{
  "name": "playground",
  "version": "99.99.99",
  "license": "WTFPL",
  "browserslist": [
    "last 2 Firefox versions",
    "last 2 Chrome versions",
    "last 2 Safari versions"
  ],
  "scripts": {
    "clean": "rm -rf dist .cache",
    "build": "parcel build index.html --public-url='/' --experimental-scope-hoisting",
    "deploy": "cp assets/robots.txt dist && rsync -vrP --del dist/ `cat location`",
    "serve": "parcel",
    "s": "parcel"
  },
  "dependencies": {
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.11.0",
    "posthtml-include": "^1.2.0",
    "posthtml-loader": "^1.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "cssnano": "^4.0.2",
    "htmlnano": "^0.1.9",
    "tslint": "^5.10.0"
  }
}
